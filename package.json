  "build": {
    "appId": "org.poooi.poi",
    "asar": true,
    "npmRebuild": false,
    "mac": {
      "publish": [],
      "icon": "assets/icons/poi.icns",
      "category": "public.app-category.games",
      "provisioningProfile": "poi.provisionprofile"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "publish": [],
      "icon": "assets/icons/poi.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "7z",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "artifactName": "poi-setup-${version}.${ext}",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "multiLanguageInstaller": true
    },
    "linux": {
      "publish": [],
      "target": [
        "7z",
        "deb",
        "rpm",
        "pacman",
        "AppImage"
      ],
      "icon": "assets/icons"
    },
    "directories": {
      "app": "app_compiled"
    }
  },
  "false": {},
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/poi",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
