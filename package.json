    "deploy": "gulp deploy",
    "build": "gulp build && build",
    "build_win": "gulp build && build --win --x64 && build --win --ia32",
    "start": "electron .",
    "start_debug": "electron . --debug",
    "clean": "gulp clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poooi/poi.git"
  },
  "keywords": [
