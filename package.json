  "build": {
    "appId": "org.poooi.poi",
    "extraResources": [
      "PepperFlash/${os}-${arch}"
    ],
    "npmRebuild": false,
    "mac": {
      "icon": "assets/icons/poi.icns",
      "target": [
        "dmg"
      ],
      "category": "public.app-category.games"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "icon": "assets/icons/poi.ico",
      "target": [
        "squirrel",
        "7z"
      ]
    },
    "linux": {
      "target": [
        "7z",
        "deb",
        "rpm",
        "pacman",
        "AppImage"
      ]
    },
    "directories": {
      "app": "app_compiled"
    }
  }
