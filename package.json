{
  "name": "poi",
  "version": "v7.10.1",
  "description": "poi the KanColle Browser Tool",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --harmony --require ./test/babelhook",
    "deploy": "gulp deploy",
    "build": "gulp build && electron-builder",
    "start": "electron .",
    "start_debug": "electron . --dev",
    "dev": "electron . --dev",
    "clean": "gulp clean",
    "lint": "eslint . --ext .es --ext .js --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poooi/poi.git"
  },
  "keywords": [
    "KanColle",
    "Browser",
    "Tool"
  ],
  "author": {
    "name": "Magica",
    "email": "readme.md@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poooi/poi/issues"
  },
  "homepage": "https://github.com/poooi/poi",
  "dependencies": {
    "@exponent/electron-cookies": "^2.0.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-preset-es2017-node7": "^0.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "classnames": "^2.2.3",
    "coffee-react": "^5.0.0",
    "coffee-script": "^1.12.7",
    "colors": "^1.1.2",
    "create-react-class": "^15.6.2",
    "cson": "^4.0.0",
    "electron-log": "^2.2.9",
    "electron-react-titlebar": "^0.5.0",
    "electron-updater": "^2.13.0",
    "fast-memoize": "^2.2.8",
    "font-awesome": "^4.7.0",
    "fs-extra": "^4.0.2",
    "fuse.js": "^3.2.0",
    "glob": "^7.0.5",
    "header-case-normalizer": "^1.0.3",
    "i18n-2": "^0.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.14.1",
    "mime": "^1.4.1",
    "moment-timezone": "^0.5.11",
    "mousetrap": "^1.5.3",
    "npm": "=5.4.2",
    "objtree": "^0.1.1",
    "pac-proxy-agent": "^2.0.0",
    "path-extra": "^4.2.1",
    "poi-asset-contributor-data": "^1.0.3",
    "poi-asset-themes": "^3.2.0",
    "poi-lib-battle": "2.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-file-dropzone": "^0.4.1",
    "react-fontawesome": "^1.6.1",
    "react-portal": "^3.0.0",
    "react-redux": "^5.0.6",
    "react-remarkable": "^1.1.3",
    "react-toastr": "^2.9.5",
    "react-virtualized": "^9.11.0",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.7.2",
    "redux-observers": "^2.0.2",
    "redux-thunk": "^2.1.0",
    "request": "^2.83.0",
    "reselect": "^3.0.0",
    "semver": "^5.4.1",
    "socks5-client": "^1.2.5",
    "socks5-http-client": "^1.0.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-eslint": "^7.0.0",
    "electron": "^1.7.9",
    "electron-builder": "^19.37.2",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "git-archive": "^0.1.4",
    "gulp": "^3.9.1",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.0.0",
    "node-7z": "^0.4.0",
    "node-unzip-2": "^0.2.1",
    "rimraf": "^2.6.2",
    "tar-fs": "^1.16.0",
    "walk": "^2.3.9"
  },
  "build": {
    "appId": "org.poooi.poi",
    "asar": false,
    "extraResources": [
      "PepperFlash/${os}-${arch}"
    ],
    "npmRebuild": false,
    "mac": {
      "publish": [
        "github"
      ],
      "icon": "assets/icons/poi.icns",
      "category": "public.app-category.games"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "publish": [
        "github"
      ],
      "icon": "assets/icons/poi.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "7z",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "artifactName": "poi-setup-${version}.${ext}",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "multiLanguageInstaller": true
    },
    "linux": {
      "publish": [
        "github"
      ],
      "target": [
        "7z",
        "deb",
        "rpm",
        "pacman",
        "AppImage"
      ]
    },
    "directories": {
      "app": "app_compiled"
    }
  },
  "false": {},
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/poi",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "optionalDependencies": {
    "7zip-bin-linux": "^1.1.0",
    "7zip-bin-mac": "^1.0.1",
    "7zip-bin-win": "^2.1.0"
  }
}
