{
  "name": "poi",
  "version": "9.0.3",
  "description": "poi the KanColle Browser Tool",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --harmony --require ./test/babelhook",
    "deploy:nightlies": "gulp deploy_nightlies",
    "build": "gulp build && electron-builder",
    "start": "electron .",
    "start_debug": "electron . --dev --inspect",
    "dev": "electron . --dev --inspect",
    "clean": "gulp clean",
    "lint:js": "eslint . --ext .es --ext .js --ignore-path .gitignore",
    "lint:css": "stylelint views/**/*.css assets/**/*.css",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poooi/poi.git"
  },
  "keywords": [
    "KanColle",
    "Browser",
    "Tool"
  ],
  "author": {
    "name": "Magica",
    "email": "readme.md@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poooi/poi/issues"
  },
  "homepage": "https://github.com/poooi/poi",
  "dependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@exponent/electron-cookies": "^2.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "@skagami/react-fontawesome": "^1.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "bindings": "^1.3.0",
    "bluebird": "^3.5.1",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "coffee-react": "^5.0.1",
    "coffee-script": "^1.12.7",
    "create-react-class": "^15.6.3",
    "cson": "^5.1.0",
    "electron-log": "^2.2.14",
    "electron-react-titlebar": "^0.8.1",
    "electron-updater": "^3.1.2",
    "fast-memoize": "^2.4.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "^7.0.0",
    "fuse.js": "^3.2.1",
    "glob": "^7.1.2",
    "header-case-normalizer": "^1.0.3",
    "i18n-2": "^0.7.2",
    "i18next": "^11.3.3",
    "immutable": "^3.8.2",
    "json-format": "^1.0.1",
    "lodash": "^4.17.10",
    "mime": "^2.3.1",
    "moment-timezone": "^0.5.21",
    "mousetrap": "^1.6.2",
    "node-fetch": "^2.1.2",
    "npm": "^6.4.1",
    "npm-package-arg": "^6.1.0",
    "pac-proxy-agent": "^3.0.0",
    "pangu": "^3.3.0",
    "path-extra": "^4.2.1",
    "poi-asset-contributor-data": "^1.1.0",
    "poi-asset-themes": "^3.3.2",
    "poi-lib-battle": "^2.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-electron-web-view": "^2.0.1",
    "react-file-dropzone": "^0.4.1",
    "react-fontawesome": "^1.6.1",
    "react-grid-layout": "^0.16.6",
    "react-i18next": "^7.7.0",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.4.0",
    "react-remarkable": "^1.1.3",
    "react-resizable-area": "^0.4.2",
    "react-ultimate-pagination": "^1.2.0",
    "react-virtualized": "^9.20.0",
    "reduce-reducers": "^0.4.3",
    "redux": "^4.0.0",
    "redux-observers": "^2.0.2",
    "redux-thunk": "^2.3.0",
    "request": "^2.87.0",
    "reselect": "^3.0.1",
    "semver": "^5.5.0",
    "socks5-client": "^1.2.6",
    "swf-extract": "^1.1.0",
    "wanakana": "^4.0.1",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "asar": "^0.14.3",
    "babel-eslint": "^10.0.0",
    "electron": "^3.0.3",
    "electron-builder": "^20.28.4",
    "eslint": "^5.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "git-archive": "^0.1.4",
    "gulp": "^4.0.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-7z": "^0.4.0",
    "node-unzip-2": "^0.2.7",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.1.0",
    "tar-fs": "^1.16.3",
    "walk": "^2.3.13"
  },
  "build": {
    "appId": "org.poooi.poi",
    "asar": true,
    "npmRebuild": true,
    "mac": {
      "publish": [
        "github"
      ],
      "icon": "assets/icons/poi.icns",
      "category": "public.app-category.games"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "publish": [
        "github"
      ],
      "icon": "assets/icons/poi.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "7z",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "artifactName": "poi-setup-${version}.${ext}",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "multiLanguageInstaller": true
    },
    "linux": {
      "publish": [
        "github"
      ],
      "target": [
        "7z",
        "deb",
        "rpm",
        "pacman",
        "AppImage"
      ],
      "icon": "assets/icons"
    },
    "directories": {
      "app": "app_compiled"
    }
  },
  "false": {},
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/poi",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
